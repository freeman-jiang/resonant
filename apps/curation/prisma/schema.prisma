generator client {
  provider             = "prisma-client-py"
  interface            = "asyncio"
  recursive_type_depth = "5"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Page {
  id            Int      @id @default(autoincrement())
  url           String   @unique
  parent_url    String?
  title         String?
  date          String?
  author        String?
  content       String
  created_at    DateTime @default(now())
  updated_at    DateTime @updatedAt
  outbound_urls String[]
}
